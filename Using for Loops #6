m = int(input("Give me a number: "))
n = int(input("Give me another number: "))

for i in range(0,n):
    for j in range(0,m):
        if i == 0 or i == n-1:
            print('*', end = '')
        else:
            if j == 0 or j == m-1:
                print("*", end='')
            else:
                print(" ", end= '')
    print("")
